{
  "manifest_version": 2,

  "name": "rick-roll",
  "description": "Simple tool for copying the rick rickk link to the clipboard",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}